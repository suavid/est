@{
    if (Session["usuario"] != null)
    {
        Layout = "~/Views/Shared/_Layout2.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_Layout3.cshtml";
    }
}

<form role="form" class="dataform">
    <h3>Ingreso de datos estadísticos - Actividad juvenil</h3>
    <hr />
    <div class="form-group">
        <label>
            Fecha
        </label>
        <input class="form-control" id="fecha" type="text" name="Fecha" ng-model="vm.FechaReporte" required>
  
        <br />
        <div class="form-group">
            <button class="btn btn-primary" ng-click="vm.ObtenerReporte('cultoDeOracion')">Buscar</button>
        </div>

    </div>
    <div class="form-group">

        <label>
            Total de asistencia
        </label>
        <input class="form-control" disabled="disabled" type="text" name="JovenesEnTemplo" ng-model="vm.ActividadJuvenil.jovenesEnTemplo" onkeyup="VerificarEntero(this);" required>
      
    
     </div>
    <div class="form-group">
        <label>
            Comentarios y observaciones
        </label>
        <textarea class="form-control" disabled="disabled" name="Comentarios"></textarea>


    </div>
  
</form>