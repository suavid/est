@{
    if (Session["usuario"] != null)
    {
        Layout = "~/Views/Shared/_Layout2.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_Layout3.cshtml";
    }
}

<form role="form" class="dataform" style="background: white; padding: 30px;">
    <div layout="row" layout-xs="column">
        <div flex style="padding: 15px;">
            <p>Fecha inicio del período</p>
            <md-datepicker ng-model="vm.FechaInicio" md-placeholder="Fecha"></md-datepicker>
            <br /><br /><br />
            <p>Fecha fin del período</p>
            <md-datepicker ng-model="vm.FechaFin" md-placeholder="Fecha"></md-datepicker>
            <br /><br /><br />
            <md-button class="md-raised md-primary" ng-click="vm.CargarReporte()">Generar reporte</md-button>
            <br /><br />
        </div>
    </div>
</form>